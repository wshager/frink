{
  "name": "frink",
  "description": "Parses XML to VTree",
  "keywords": [
    "xml",
    "vtree",
    "vnode",
    "virtual dom",
    "json",
    "immutable"
  ],
  "version": "0.2.0",
  "author": "Wouter Hager <wshager@gmail.com> (https://lagua.nl)",
  "contributors": [],
  "main": "./lib/index",
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "build": "babel src --out-dir lib --ignore layers",
    "client-build": "browserify src/layers/frink.js -s amd -o dist/frink.js -t [ babelify --presets [ es2015 ] ]",
    "client-min": "uglifyjs --compress --mangle --verbose dist/frink.js -o dist/frink.min.js",
    "form-build": "browserify src/layers/frinkiform.js -s amd -o dist/frinkiform.js -t [ babelify --presets [ es2015 ] ]",
    "form-min": "uglifyjs --compress --mangle --verbose dist/frinkiform.js -o dist/frinkiform.min.js",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wshager/frink.git"
  },
  "dependencies": {
    "big.js": "^3.1.3",
    "entries": "^1.0.1",
    "events": "^1.1.1",
    "ohamt": "*",
    "rrb-vector": "*",
    "sax": "*",
    "xregexp": "^3.1.1"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/wshager/frink/issues"
  },
  "homepage": "https://github.com/wshager/frink#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "uglify-js": "^2.8.22"
  }
}
